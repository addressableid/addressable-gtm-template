___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const callInWindow = require('callInWindow');
const injectScript = require('injectScript');
const JSON = require('JSON');
const encodeUriComponent = require('encodeUriComponent');

log("data: " + JSON.stringify(data));

var pixel_id = data.pixel_id;
var event_name = data.standard_event_name;
if (data.event_name == "custom"){
  event_name = data.custom_event_name;
}
var is_conversion = data.is_conversion;
var object_property_list = data.object_property_list;

// Inject a script tag to load pixel.js.
const url = "https://alpha-tag.adrsbl.io/p.js?tid=" + encodeUriComponent(pixel_id);
injectScript(url, run, data.gtmOnFailure, "pixel");

function run () {
  callInWindow('__adrsbl.run', event_name, is_conversion , object_property_list);
  data.gtmOnSuccess();
}


